                                if($iii == 3 ){  

                                  if(strlen($price_csv[$iii][$kk]) >= 71){
                                    $fontSize = 'titleSmall';
                                  }
                                  else{
                                    $fontSize ='titleNormal';
                                  }
                                $plan[$iii] .= '<div class="hotelPlanTitle' . $iii . '_' . $kk . ' '. $fontSize  . ' '.  strlen($price_csv[$iii][$kk])  .'"><p>' . nl2br($price_csv[$iii][$kk])   . '</p>' .'</div>';

