                                  }
                                  else{
                                    $plan[$iii] .= '<div class="textCenter hotelPlanPrice' . $iii . '_' . $kk . '"><p> - </p></div>';
                                  }
                                }
                              }

                          }
                          if(!empty($plan[$iii])){
                            $plans .= '<div class="price-tbl-col">'. $plan[$iii] .'</div>';
                          }
